type BingoCard {
  numbers: [Int!]!
}

type ValidateResult {
  row: [Int]!
  col: [Int]!
  diagonal: [Int]!
	numbers: [Int!]!
  isValid: Boolean!
}

extend type Query {
  bingoCard: BingoCard!
  validateCard(id: String!): ValidateResult!
}

extend type Mutation {
  addLotteryNumber(number: Int!): Int!
  resetLottery: Boolean!
}
